<%= render 'user_error', user: @user %>

<%= render 'user_info', user: @user %>

<%= render 'user_edit_link', user: @user %>

<%= render 'books/new_book', book: @book %>


<% @users.each do |user| %>
<div>
  <h1>Users</h1>
  <table class="table">
    <tbody>
      <tr>
        <th>image</th>
        <th>名前</th>
      </tr>
      <tr>
        <th><%= image_tag user.get_profile_image(100,100) %></th>
        <th><%= user.name %></th>
      </tr>
      <tr>
        <th><%= link_to "Show", user_path(user.id) %></th>
      </tr>
    </tbody>
  </table>
</div>
<% end %>